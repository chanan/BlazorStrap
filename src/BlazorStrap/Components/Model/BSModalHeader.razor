@namespace BlazorStrap
@inherits BSModalHeaderBase

<DynamicElement @attributes="@UnknownParameters" TagName="div" class="modal-header">
    <h5 class="@classname">@ChildContent</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close" @onclick="OnClick" autofocus>
        <span aria-hidden="true">&times;</span>
    </button>
</DynamicElement>