@namespace BlazorStrap
@inherits CodeBSTab;

<CascadingValue Value="@this">
    @if (Parent.IsList)
    {
    <DynamicElement @attributes="@UnknownParameters" TagName="il" class="@classname" >
        @ChildContent
    </DynamicElement>
    }
    else
    {
        @ChildContent
    }
</CascadingValue>
