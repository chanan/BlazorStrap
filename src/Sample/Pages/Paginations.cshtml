@addTagHelper *, BlazorStrap
@page "/pagination"

<h1>Pagination</h1>

<div class="docs-example">
    <Pagination>
        <PaginationItem>
            <PaginationLink Href="#" PaginationLinkType="PaginationLinkType.PreviousText" />
        </PaginationItem>
        <PaginationItem>
            <PaginationLink Href="#" PaginationLinkType="PaginationLinkType.Custom">1</PaginationLink>
        </PaginationItem>
        <PaginationItem>
            <PaginationLink Href="#" PaginationLinkType="PaginationLinkType.Custom">2</PaginationLink>
        </PaginationItem>
        <PaginationItem>
            <PaginationLink Href="#" PaginationLinkType="PaginationLinkType.Custom">3</PaginationLink>
        </PaginationItem>
        <PaginationItem>
            <PaginationLink Href="#" PaginationLinkType="PaginationLinkType.NextText" />
        </PaginationItem>
    </Pagination>
</div>
